<mvc:View
	controllerName="sap.ui.demo.walkthrough.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
>
	<Shell>
		<App>
			<pages>
			<Page
		showHeader="true"
		title="Tela Inicial"
		showFooter="true"
		showNavButton="false"
	>
		<content>
			<List
				id="sap_Responsive_Page_0-content-sap_m_StandardList-1685105998995"
				mode="SingleSelectMaster"
				width="100%"
				headerText=""
				footerText=""
				backgroundDesign="Solid"
				showSeparators="All"
				growing="true"
				growingThreshold="20"
				growingScrollToLoad="true"
				visible="true"
				class="sapUiResponsiveContentPadding"
				itemPress="_onStandardListItemPress"
				items="{
					path: '/O_AlunoSet',
					templateShareable: true
				}"
			>
				<infoToolbar>
					<Toolbar
						width="100%"
						height="auto"
						design="Auto"
						visible="false"
						enabled="true"
					>
						<content>
							<Label
								text="Label"
								design="Standard"
								width="100%"
								required="false"
								textAlign="Begin"
								textDirection="Inherit"
								visible="true"
							/>
						</content>
					</Toolbar>
				</infoToolbar>
				<headerToolbar/>
				<items>
					<StandardListItem
						icon="sap-icon://person-placeholder"
						title="{Nome}"
						description="{
							path: 'Nascimento',
							type: 'sap.ui.model.type.Date',
							formatOptions: {
								UTC: true,
								style: 'short'
							}
						}"
						info="{ID}"
						infoState="None"
						type="Active"
						selected="false"
						visible="true"
						press="_onStandardListItemPress1"
					/>
				</items>
			</List>
		</content>
		<footer/>
		<headerContent/>
		<subHeader>
			<OverflowToolbar
				visible="true"
				enabled="true"
				width="auto"
				design="Auto"
			>
				<content>
					<SearchField
						placeholder="Search"
						showSearchButton="true"
						visible="true"
						width="75%"
						class="sapUiResponsiveMargin"
						search="_onSearchFieldSearch"
					/>
					<Button
						text=" novo aluno"
						type="Emphasized"
						icon="sap-icon://write-new-document"
						iconFirst="true"
						width="auto"
						enabled="true"
						visible="true"
						iconDensityAware="false"
						class="sapUiResponsiveMargin"
					/>
				</content>
			</OverflowToolbar>
		</subHeader>
		<customHeader/>
	</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>