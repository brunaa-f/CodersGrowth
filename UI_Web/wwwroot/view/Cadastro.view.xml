<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="SimpleFormChange354"
			editable="true"
			layout="ResponsiveGridLayout"
         title="{i18n>tituloCadastro}"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			columnsXL="1"
			columnsL="1"
			
			singleContainerFullSize="false" >
			<f:content>
				<Label text="{i18n>labelNome}" />
				<Input  value="{aluno>nome}" />
            <Label text="{i18n>labelCpf}" />
				<Input  value="{}" />
            <Label text="{i18n>labelTelefone}" />
				<Input value="{}" />
            <Label text="{i18n>labelDataNascimento}" labelFor="DataNascimento"/>
		      <DatePicker placeholder="Enter Date" class="sapUiSmallMarginBottom" value="{}"/>
			
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>
